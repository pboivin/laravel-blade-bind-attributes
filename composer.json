{
    "name": "pboivin/laravel-blade-bind-attributes",
    "description": "Experimental",
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pboivin\\LaravelBladeBindAttributes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pboivin\\LaravelBladeBindAttributes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pboivin\\LaravelBladeBindAttributes\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "@php vendor/bin/phpunit"
        ],
        "format": [
            "@php vendor/bin/pint"
        ]
    }
}
