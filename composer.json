{
    "name": "pboivin/laravel-blade-bind-attributes",
    "description": "Attribute unpacking for Laravel Blade component tags",
    "keywords": ["php", "laravel", "blade", "package"],
    "homepage": "https://github.com/pboivin/laravel-blade-bind-attributes",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pboivin/laravel-blade-bind-attributes/issues",
        "source": "https://github.com/pboivin/laravel-blade-bind-attributes"
    },
    "authors": [
        {
            "name": "Patrick Boivin",
            "email": "pboivin@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Pboivin\\LaravelBladeBindAttributes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app/",
            "Pboivin\\LaravelBladeBindAttributes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pboivin\\LaravelBladeBindAttributes\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "@php vendor/bin/phpunit"
        ],
        "format": [
            "@php vendor/bin/pint"
        ]
    }
}
